<h1>Dashboard</h1>

<h2>Low Stock Products</h2>
<% if @low_stock_products.any? %>
  <%= render @low_stock_products %>
<% else %>
  <p>No products with low stock.</p>
<% end %>

<h2>Recent Orders</h2>
<% if @recent_orders.any? %>
  <%= render @recent_orders %>
<% else %>
  <p>No recent orders.</p>
<% end %>
